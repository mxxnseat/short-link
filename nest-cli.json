{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/short-link/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/short-link/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/short-link",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "short-link": {
      "type": "application",
      "root": "apps/short-link",
      "entryFile": "main",
      "sourceRoot": "apps/short-link/src",
      "compilerOptions": {
        "tsConfigPath": "apps/short-link/tsconfig.app.json"
      }
    },
    "websocket-server": {
      "type": "application",
      "root": "apps/websocket-server",
      "entryFile": "main",
      "sourceRoot": "apps/websocket-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/websocket-server/tsconfig.app.json"
      }
    }
  }
}
